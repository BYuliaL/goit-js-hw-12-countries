{{#each this}}
<div class="card">
    <div class="card-info">

        <h1 class="country_name">{{name}}</h1>
        <h3 class="capital">Capital: {{capital}}</h3>
        <h3 class="population">Population: {{population}}</h3>

        <ul class="languages">
            <h3>Languages: {{#each languages}}</h3>
            <li class="language_item">{{this.name}}</li>
            {{/each}}
    </div>
    </ul>
    <img class="flag" src="{{flag}}" alt="This is flag of the country" width="300">
</div>
{{/each}}