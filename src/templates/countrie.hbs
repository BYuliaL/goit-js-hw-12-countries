{{#each this}}
<div class="card">
    <div class="card-info">

        <h1 class="country_name">{{name}}</h1>
        <p class="capital">Capital: {{capital}}</p>
        <p class="population">Population: {{population}}</>

        <ul class="languages">
            <p>Languages: {{#each languages}}</p>
            <li class="language_item">{{this.name}}</li>
            {{/each}}
    </div>
    </ul>
    <img class="flag" src="{{flag}}" alt="This is flag of the country" width="300">
</div>
{{/each}}